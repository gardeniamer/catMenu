
.all {
	margin: 10px auto 10px auto;
	width: 700rpx;
	height: 500rpx;
}
.classification {
	width: 100%;
	height: 30%;
	display: flex;
	flex-direction: row;
	justify-content: space-around;
	align-items: center;
}
.refer {
	margin: 10px 0 10px 0;
	width: 100%;
	height: 66%;
}
.various {
	width: 120rpx;
	height: 100%;
}
.various image {
	width: 45px;
	height: 45px;
	-webkit-transform: translateX(8px);
	        transform: translateX(8px);
}
.iconText {
	margin: 5px 0 0 0;
	text-align: center;
	font-size: 14px;
	font-weight: 500;
}
.referPhoto {
	margin: 0 auto;
	width: 700rpx;
	height: 350rpx;
	-webkit-transform: translateX(0px);
	        transform: translateX(0px);
}


