
.positionAll {
	width: 100%;
	height: 120rpx;
	position: relative;
	background-color: #fff;
}
.innerContent {
	width: 95%;
	height: 88%;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
}
.star {
	width: 25px;
	height: 24px;
	vertical-align: middle;
}
.positionDetail {
	display: inline-block;
	-webkit-transform: translate(10px,2px);
	        transform: translate(10px,2px);
	font-size: 15px;
}
.position {
	width: 12px;
	height: 16px;
	-webkit-transform: translate(15px,3px);
	        transform: translate(15px,3px);
}
.positionDetails {
	display: inline-block;
	font-size: 8px;
	-webkit-transform: translate(20px);
	        transform: translate(20px);
	color: #a9a9a9;
}
.choose {
	width: 200rpx;
	height: 50rpx;
	position: absolute;
	top: 50%;
	left: 65%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	border-radius: 60rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	background-color: #f0f0f0;
}
.getSelf,.getOther {
	width: 100rpx;
	height: 50rpx;
	border-radius: 60rpx;
	text-align: center;
	line-height: 50rpx;
	font-size: 14px;
}
.chooseMethod {
	background-color: #95d6bf;
}

